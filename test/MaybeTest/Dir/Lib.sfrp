import STDLib

type Maybe[a] = Just(a) | Nothing
type Tuple2[a, b] = Tuple2(a, b)

foreign putInt as $putInt(Int) : Unit

foreign getMaybeIntRaw as $getMaybeIntRaw() : (Bool, Int)
$getMaybeInt() : Maybe[Int] = case $getMaybeIntRaw() of
  (True, x) -> Just(x)
  (False, _) -> Nothing

getOrElse(m, v) = case m of
  Just(x) -> x
  Nothing -> v
